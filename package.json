{
  "name": "standard-prettier-eslint",
  "version": "1.0.2",
  "description": "Code -> prettier -> standard --fix -> Formatted Code",
  "keywords": [
    "prettier",
    "eslint",
    "prettier-eslint",
    "linter",
    "standard",
    "standard-engine",
    "semistandard"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "format": "prettier-eslint --write '**/*.js' --eslint-path=."
  },
  "dependencies": {
    "standard-cliengine": "1.x.x",
    "standard": "*"
  },
  "devDependencies": {
    "snazzy": "*",
    "prettier-eslint-cli": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bySabi/standard-prettier-eslint.git"
  },
  "bugs": {
    "url": "https://github.com/bySabi/standard-prettier-eslint/issues"
  },
  "homepage": "https://github.com/bySabi/standard-prettier-eslint#readme",
  "author": "bySabi Files",
  "license": "MIT"
}
